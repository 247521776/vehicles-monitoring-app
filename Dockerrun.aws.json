{
  "AWSEBDockerrunVersion": 2,

  "volumes": [{
    "name": "ui",
    "host": {
      "sourcePath": "/var/app/current/vehicle-monitoring-ui"
    }
  },
    {
      "name": "proxy",
      "host": {
        "sourcePath": "/var/app/current/etc/nginx"
      }
    },
    {
      "name": "app",
      "host": {
        "sourcePath": "/opt/app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name":"client",
      "image":"abdotalaat/test-client",
      "hostname":"client",
      "essential": false,
      "memory":128,
      "portMappings":[
        {
          "hostPort":8080,
          "containerPort":8080
        }
      ]
    },

    {
      "name":"customer",
      "image":"abdotalaat/customer-service",
      "hostname":"customer",
      "essential": false,
      "memory":128,
      "portMappings":[
        {
          "hostPort":9010,
          "containerPort":9010
        }
      ]
    },

    {
      "name":"ngnix",
      "image":"abdotalaat/multi-nginx",
      "hostname":"nginx",
      "essential": true,
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "links":
      [
        "discovery","client"
      ],
      "memory":128
    }





  ]
}